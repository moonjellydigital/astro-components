{
  "name": "@moonjellydigital/astro-components",
  "description": "A collection of accessible UI components for Astro",
  "version": "0.1.0",
  "main": "./index.js",
  "files": [
    "./index.js",
    "./SkipLink.astro",
    "./Heading.astro",
    "./StateLayer.astro",
    "./DisclosureButton.astro",
    "./ToggleButton.astro",
    "./BackToTop.astro"
  ],
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "Camille Spain <camille@camillespain.com>",
  "license": "MIT",
  "keywords": [
    "astro",
    "astro components",
    "astro component",
    "ui components",
    "ui library",
    "accessible",
    "accessibility",
    "a11y",
    "wcag",
    "responsive"
  ],
  "homepage": "https://github.com/moonjellydigital/astro-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonjellydigital/astro-components.git"
  },
  "scripts": {
    "test": "vitest --run",
    "prepare": "husky || true",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "tsc:ci": "astro check",
    "fmt:ci": "bunx prettier --check .",
    "lint:js:ci": "bunx eslint .",
    "lint:css:ci": "bunx stylelint **/*.css"
  },
  "lint-staged": {
    "*.{json,yml,yaml,md}": "prettier --write",
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.astro": "prettier --write"
  },
  "type": "module",
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.2",
    "@eslint/js": "^9.12.0",
    "@moonjellydigital/prettier-config": "^1.0.0",
    "@types/bun": "latest",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/prettier": "^3.0.0",
    "@types/throttle-debounce": "^5.0.2",
    "astro": "^4.16.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.4.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "throttle-debounce": "^5.0.2"
  }
}
