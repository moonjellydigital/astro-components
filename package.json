{
  "name": "@moonjellydigital/astro-components",
  "description": "A collection of accessible UI components for Astro",
  "version": "0.2.0",
  "main": "./index.js",
  "files": [
    "./index.js",
    "./modules/**/*.ts",
    "./modules/**/*.map",
    "./modules/**/*.js",
    "./SkipLink.astro",
    "./Heading.astro",
    "./StateLayer.astro",
    "./DisclosureButton.astro",
    "./ToggleButton.astro",
    "./BackToTop.astro",
    "./Icon.astro"
  ],
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "Camille Spain <camille@camillespain.com>",
  "license": "MIT",
  "keywords": [
    "astro",
    "astro components",
    "astro component",
    "ui components",
    "ui library",
    "accessible",
    "accessibility",
    "a11y",
    "wcag",
    "responsive"
  ],
  "homepage": "https://github.com/moonjellydigital/astro-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonjellydigital/astro-components.git"
  },
  "scripts": {
    "test": "bunx vitest --run",
    "prepare": "husky || true",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "tsc:ci": "astro check",
    "fmt:ci": "bunx prettier --check .",
    "lint:js:ci": "bunx eslint .",
    "lint:css:ci": "bunx stylelint **/*.css",
    "build:modules": "bunx tsc --build tsconfig.build.json",
    "test:browser": "vitest --workspace=vitest.workspace.browser.ts"
  },
  "lint-staged": {
    "*.{json,yml,yaml,md}": "prettier --write",
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.astro": "prettier --write"
  },
  "type": "module",
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.20.0",
    "@moonjellydigital/prettier-config": "^1.0.0",
    "@types/bun": "latest",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/prettier": "^3.0.0",
    "@types/throttle-debounce": "^5.0.2",
    "astro": "5.2.5",
    "esbuild": "^0.24.2",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "3.0.0-beta.3"
  },
  "dependencies": {
    "throttle-debounce": "^5.0.2"
  }
}
